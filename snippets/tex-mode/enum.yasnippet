# -*- mode: snippet -*-
# name: enum
# key: enum
# --
\begin{enumerate}
`(let ((counter (string-to-number (read-string "Enter the range (1-n): "))))
   (cl-loop for i from 1 to counter concat
              (if (< i counter)
   	               (format "\\item\n")
            	       (format "\\item"))))`
\end{enumerate}